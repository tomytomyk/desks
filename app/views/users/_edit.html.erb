<%= form_for user do |f| %>
  名前<%= f.text_field :name %>
  パスワード<%= f.password_field :password %>
  職種<%= f.collection_select(:occupation_id, Occupation.all, :id, :name) %>
  イメージ画像<%= f.file_field :image_id %>
  <%= f.submit %>
<% end %>
<%= link_to "退会する" ,user_path(user.id), method: :delete, "data-confirm" => "本当に退会しますか？" %>