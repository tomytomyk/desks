<div id="iframe_id">
<h1>商品一覧</h1>
<% @items.each do |item| %>
    <%= item.id %>
	<%= item.name %>
	<%= item.image_id %>
	<%= item.price %>
	<%= item.maker.name %>
	<%= item.genre.name %>
	<%= link_to "編集", edit_admin_item_path(item.id), remote: true %>
	<div id="result"></div>
	<%= link_to "消去", admin_item_path(item.id),method: :delete %><br>

<% end %>
<div class="add-item"></div>
</div>
